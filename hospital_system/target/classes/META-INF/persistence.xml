<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd"
             version="3.0">
    <persistence-unit name="default">
        <description>DBS CV05 Application</description>
        <class>dbs.entities.AssistantEntity</class>
        <class>dbs.entities.DoctorEntity</class>
        <class>dbs.entities.HospitalEntity</class>
        <class>dbs.entities.MedicalRecordEntity</class>
        <class>dbs.entities.OperationEntity</class>
        <class>dbs.entities.OperationRoomEntity</class>
        <class>dbs.entities.PatientEntity</class>
        <class>dbs.entities.VenueEntity</class>
        <class>dbs.entities.CountOfAssistantsPerHospitalEntity</class>
        <class>dbs.entities.CountOfDoctorsPerHospitalEntity</class>
        <class>dbs.entities.Workers1Entity</class>
        <exclude-unlisted-classes>true</exclude-unlisted-classes>
        <properties>
            <property name="hibernate.hbm2ddl.auto" value="update"/>
            <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
            <property name="javax.persistence.jdbc.url" value=""/>
            <property name="javax.persistence.jdbc.user" value=""/>
            <property name="javax.persistence.jdbc.password" value=""/>
        </properties>
    </persistence-unit>
</persistence>
